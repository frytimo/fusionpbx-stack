<!-- Codecs -->
<X-PRE-PROCESS cmd="set" data="global_codec_prefs=PCMU" />
<X-PRE-PROCESS cmd="set" data="outbound_codec_prefs=G722,PCMU,G729" />
<X-PRE-PROCESS cmd="set" data="media_mix_inbound_outbound_codecs=true" />

<!-- DSN -->
<X-PRE-PROCESS cmd="set" data="dsn_callcenter=sqlite:///dev/shm/callcenter.db" />
<X-PRE-PROCESS cmd="set" data="dsn_system=pgsql://hostaddr=db.local port=5432 dbname=fusionpbx user=fusionpbx password=fusionpbx" />
<X-PRE-PROCESS cmd="set" data="dsn=pgsql://hostaddr=db.local port=5432 dbname=fusionpbx user=fusionpbx password=fusionpbx" />

<!-- Defaults -->
<X-PRE-PROCESS cmd="set" data="default_areacode=613" />
<X-PRE-PROCESS cmd="set" data="default_country=CA" />
<X-PRE-PROCESS cmd="set" data="default_language=en" />
<X-PRE-PROCESS cmd="set" data="default_dialect=us" />
<X-PRE-PROCESS cmd="set" data="default_voice=callie" />
<X-PRE-PROCESS cmd="set" data="ajax_refresh_rate=3000" />
<X-PRE-PROCESS cmd="set" data="ringback=$${us-ring}" />
